{
  "name": "kuiz-vue",
  "version": "1.0.0",
  "description": "Student-Generated Multiple Choice Questions | Project Remote Learning",
  "author": "Elliot Jung, Inhwa Song",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=local nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "cross-env NODE_ENV=production nuxt generate",
    "deploy:staging": "cross-env NODE_ENV=staging nuxt generate && aws s3 sync ./dist s3://staging.kuiz.kixlab.org && aws cloudfront create-invalidation --distribution-id E30GH6RGFO9YS --paths \"/*\"",
    "deploy:production": "cross-env NODE_ENV=production nuxt generate && aws s3 sync ./dist s3://kuiz.kixlab.org && aws cloudfront create-invalidation --distribution-id E2E8RHO1YRN3PF --paths \"/*\"",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "test": "jest"
  },
  "sass": {
    "includePaths": [
      "./node_modules"
    ]
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "core-js": "^3.15.1",
    "nuxt": "^2.15.7",
    "v-click-outside": "^3.1.2",
    "v-tooltip": "^2.1.3",
    "vue-radial-progress": "^0.3.2",
    "vue2-animate": "^2.1.4",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@vue/test-utils": "^1.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.0.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.29.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "jest": "^27.0.5",
    "node-sass": "^6.0.1",
    "prettier": "^2.3.2",
    "push-dir": "^0.4.1",
    "sass-loader": "10",
    "vue-jest": "^3.0.4"
  }
}
